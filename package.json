{
  "name": "",
  "version": "0.0.0",
  "description": "TODO",
  "main": ".js",
  "scripts": {
    "build": "npm run build:src && npm run uglify && npm run html",
    "build:src": "npm run build:js && npm run build:css && npm run html:src",
    "html": "cp templates/index-production.html public/index.html",
    "html:src": "cp templates/index-dev.html public/index.html",
    "build:js": "browserify --bare -t [ babelify --presets [ @babel/preset-env ] ] src/js/index.js -o public/bundle.js",
    "watch:js": "watchify --bare --debug --verbose -t [ babelify --presets [ @babel/preset-env ] ] src/js/index.js -o public/bundle.js",
    "build:css": "stylus -u autoprefixer-stylus -u nib src/css/index.styl -o public/bundle.css",
    "watch:css": "stylus -w -u autoprefixer-stylus -u nib src/css/index.styl -o public/bundle.css",
    "prewatch": "npm run html:src && npm run vendor:src",
    "watch": "miru -p public -w [ npm run watch:js -o public/bundle.js -r bytes.written ] -w [ npm run watch:css -o public/bundle.css ] -t public/index.html -f templates/index-dev.html -e 'npm run html:src'",
    "uglify": "npm run uglify:js && npm run uglify:css",
    "uglify:js": "uglifyjs public/bundle.js -cmo public/bundle.min.js",
    "uglify:css": "uglifycss public/bundle.css --output public/bundle.min.css",
    "vendor": "npm run vendor:src && npm run vendor:min",
    "vendor:min": "concat vendor/min/* -o public/vendor.js && uglifyjs public/vendor.js -cmo public/vendor.min.js",
    "vendor:src": "mkdir public/vendor || cp vendor/src/* public/vendor/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    ""
  ],
  "author": "talmobi <talmo.christian@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/talmobi/"
  },
  "bugs": {
    "url": "https://github.com/talmobi//issues",
    "email": "talmo.christian@gmail.com"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-stylus": "~0.14.0",
    "@babel/core": "~7.7.5",
    "@babel/preset-env": "~7.7.6",
    "babelify": "~8.0.0",
    "browserify": "~16.2.2",
    "concat": "~1.0.3",
    "miru": "~0.13.0",
    "nib": "~1.1.2",
    "stylus": "~0.54.5",
    "uglify-js": "~3.4.7",
    "uglifycss": "0.0.29",
    "watchify": "~3.11.0"
  }
}
